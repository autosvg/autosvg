{
  "parser": "babel-eslint",
  "rules": {
      "indent": [
          1,
          2
      ],
      "quotes": [
          2,
          "double"
      ],
      "linebreak-style": [
          2,
          "unix"
      ],
      "semi": [
          2,
          "always"
      ],
      "no-console": [
        1
      ],
      "no-unused-vars": [
        1, {
          "args": "none"
        }
      ],
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "implicitStrict": true
    }
  },
  "env": {
    "es6": true,   
    "browser": true
  },
  "globals": {
    "log": false
  }
}
