finite-automaton {
  states {
    q0[label="q<tspan baseline-shift=sub>0</tspan>" initial terminal]
    q1[label="q<tspan baseline-shift=sub>1</tspan>"]
    q2[label="q<tspan baseline-shift=sub>2</tspan>"]
    q3[label="q<tspan baseline-shift=sub>3</tspan>"]
  }
  transitions {
    q0 1 -> q1
    q1 1 -> q0
    q0 0 -> q2
    q2 0 -> q0
    q1 0 -> q3
    q3 0 -> q1
    q2 1 -> q3
    q3 1 -> q2
  }
}
